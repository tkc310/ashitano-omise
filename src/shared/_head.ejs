<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="format-detection" content="telephone=no">

  <!-- SEO start -->
  <meta name="robots" content="noindex">

  <%
  var title = data.title;
  if (data.title) {
    title += ' | ';
  }
  title += global.site_name;
  %>
  <title><%= title %></title>
  <meta name="description" content="<%= data.description %>">
  <meta name="keywords" content="<%= data.keywords %>">

  <meta property="og:title" content="<%= title %>">
  <meta property="og:type" content="website">
  <meta property="og:image" content="<%= global.host %>/assets/images/<%= data.og_image || global.og_image %>">
  <meta property="og:url" content="<%= global.host %>/<%= data.url %>">

  <link rel="canonical" href="<%= global.host %>/<%= data.url %>">
  <!-- SEO end -->

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Noto+Sans+JP" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Roboto&display=swap" rel="stylesheet">

  <!-- Google Material design icons -->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

  <link rel="stylesheet" type="text/css" href="/assets/css/app.css">
</head>
