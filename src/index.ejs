          ].forEach(function(shop) { %>
          <li class="c-list_item">
            <div class="c-list_img-wrap">
              <img class="c-list_img"
                   src="/assets/images/<%= shop.img %>"
                   alt="<%= shop.title %>">
            </div>
            <div class="c-list_desc">
              <time class="c-list_date">
                <%= shop.date %>
              </time>
              <h3 class="c-list_title">
                <%= shop.title %>
              </h3>
              <p class="c-list_excerpt u-elps-3">
                <%= shop.excerpt %>
              </p>
              <div class="c-list_more">
                <a class="c-list_more-link"
                   href="/shops/<%= shop.id %>">
                  READ MORE >
                </a>
              </div>
            </div>
          </li>
          <% }); %>
        </ul>
      </section>
    </div>
  </main>

  <footer class="l-footer">
    <div class="l-footer_head">
      <p class="l-footer_copyright">
        Ashitano Omise Tokyo
      </p>
      <div id="vue-btn-top"></div>
