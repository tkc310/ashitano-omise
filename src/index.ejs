<body>
<div class="l-container">
  <header class="l-header">
    <h1 class="l-header_logo">Ashitano Omise Tokyo</h1>
  </header>

  <main class="l-content">
    <div class="p-index">
      <section class="p-index_shops">
        <!--<div class="p-index_shops-title c-heading">-->
          <!--<i class="c-heading_icon material-icons">fiber_new</i>-->
          <!--<span class="c-heading_text">LATED SHOPS</span>-->
        <!--</div>-->
        <ul class="c-list">
          <% [
            { id: 1, title: '臨時休業のお知らせ', excerpt: '概要あのイーハトーヴォの透き通った風...', date: '2019.12.24', img: 'img_01.jpg' },
            { id: 2, title: '臨時休業のお知らせ', excerpt: '概要あのイーハトーヴォの透き通った風...', date: '2019.12.24', img: 'img_02.jpg' },
            { id: 3, title: '臨時休業のお知らせ', excerpt: '概要あのイーハトーヴォの透き通った風...', date: '2019.12.24', img: 'img_03.jpg' },
            { id: 4, title: '臨時休業のお知らせ', excerpt: '概要あのイーハトーヴォの透き通った風...', date: '2019.12.24', img: 'img_04.jpg' },
            { id: 5, title: '臨時休業のお知らせ', excerpt: '概要あのイーハトーヴォの透き通った風...', date: '2019.12.24', img: 'img_05.jpg' },
            { id: 6, title: '臨時休業のお知らせ', excerpt: '概要あのイーハトーヴォの透き通った風...', date: '2019.12.24', img: 'img_06.jpg' },
            { id: 7, title: '臨時休業のお知らせ', excerpt: '概要あのイーハトーヴォの透き通った風...', date: '2019.12.24', img: 'img_07.jpg' },
            { id: 8, title: '臨時休業のお知らせ', excerpt: '概要あのイーハトーヴォの透き通った風...', date: '2019.12.24', img: 'img_08.jpg' },
            { id: 9, title: '臨時休業のお知らせ', excerpt: '概要あのイーハトーヴォの透き通った風...', date: '2019.12.24', img: 'img_09.jpg' },
          ].forEach(function(shop) { %>
          <li class="c-list_item">
            <div class="c-list_img-wrap">
              <img class="c-list_img"
                   src="/assets/images/<%= shop.img %>"
                   alt="<%= shop.title %>">
            </div>
            <div class="c-list_desc">
              <time class="c-list_date">
                <%= shop.date %>
              </time>
              <div class="c-list_title">
                <%= shop.title %>
              </div>
              <div class="c-list_excerpt">
                <%= shop.excerpt %>
              </div>
              <div class="c-list_more">
                <a class="c-list_more-link"
                   href="/shops/<%= shop.id %>">
                  READ MORE >
                </a>
              </div>
            </div>
          </li>
          <% }); %>
        </ul>
      </section>
    </div>
  </main>

  <footer class="l-footer">
    <div class="l-footer_head">
      <p class="l-footer_copyright">
        Ashitano Omise Tokyo
      </p>
      <button class="l-footer_btn-top c-btn">
        <i class="c-btn_icon material-icons">arrow_drop_up</i>
        <span class="l-footer_btn-top-text c-btn_text">BACK TO TOP</span>
      </button>
    </div>
  </footer>
</div>

